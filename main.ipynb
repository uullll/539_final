    "# Load and Preprocess Data\n",
    "set_seed()\n",
    "df = get_df(\"ISIC_2020_Training_GroundTruth_PatientLabels.csv\")\n",
    "\n",
    "train_df, val_df, test_df = group_split_df(df, test_size=0.2)\n",
    "train_transform, val_transform, test_transform = transforms(img_size=224)\n",
    "\n",
    "train_ds = PatientDS(train_df, train_transform, image_path = 'training_data/jpg', use_meta=True, mode='Train', saving_images=False)\n",
    "val_ds = PatientDS(val_df, val_transform, image_path = 'training_data/pth', use_meta=True, mode='Train', saving_images=False)\n",
    "test_ds = PatientDS(test_df, test_transform, image_path = 'training_data/pth', use_meta=True, mode='Test', saving_images=False)\n",
    "\n",
    "sampler = PatientLevelSampler(train_ds, pos_fraction=0.5)\n",
    "# sampler_test = PatientLevelSampler(test_ds, pos_fraction=0.5)\n",
    "\n",
    "train_dl = DataLoader(train_ds, batch_size=1,  sampler=sampler, shuffle = False, num_workers=3)\n",
    "val_dl = DataLoader(val_ds, batch_size=1,  sampler=None, shuffle=False, num_workers=0)\n",
    "test_dl = DataLoader(test_ds, batch_size=1,  sampler=None, shuffle=False, num_workers=0)"
   ],
